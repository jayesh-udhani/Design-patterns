
public class Boeing747Builder extends AircraftBuilder {

	private Aircraft boeing747;
	
	public Boeing747Builder() {
		// TODO Auto-generated constructor stub
		boeing747 = new Boeing747();
	}
	
	@Override
	public void buildEngine() {
		// TODO Auto-generated method stub
		boeing747.setEngine("Buidling engine of Boeing 747");
	}

	@Override
	public void buildWings() {
		// TODO Auto-generated method stub
		boeing747.setWings("Building wings of Boeing 747");
	}

	@Override
	public void buildCockpit() {
		// TODO Auto-generated method stub
		boeing747.setCockpit("Building cockpit of Boeing 747");
	}

	@Override
	public void buildBathrooms() {
		// TODO Auto-generated method stub
		boeing747.setBathrooms("Building bathrooms of Boeing 747");
	}
	
	public Aircraft getResult() {
		return boeing747;
	}

}
