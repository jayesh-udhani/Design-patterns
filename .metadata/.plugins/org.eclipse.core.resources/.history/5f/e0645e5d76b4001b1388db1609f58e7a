
public abstract class Aircraft {
	
	public abstract String getEngine();
	
	public abstract void setEngine(String engine);
	
	public abstract String getWings();
	
	public abstract void setWings(String wings);
	
	public abstract String getCockpit();
	
	public abstract void setCockpit(String cockpit);
	
	public void setBathrooms(String bathrooms) {}
	
}
